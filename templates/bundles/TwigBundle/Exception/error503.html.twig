{% extends 'base.html.twig' %}

{% block title %}Erreur serveur{% endblock %}

{% block body %}
<section class="container">
    <div class="text-center">
        <h1 class="text-center">Erreur serveur</h1>
        <p class="text-center">La page que vous cherchez n'a pas été trouvée !</p>
        <p>Code de l'erreur : {{ status_code }}</p>
        <p>Message de l'erreur : {{ status_text }}</p>
    </div>
</section>
{% endblock %}